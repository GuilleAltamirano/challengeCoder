<link rel="stylesheet" type="text/css" href="/css/realtimeproducts.css">

<h2 class="titles">Products</h2>
<div class="products-container">
    {{#each products}}
        <div class="product">
            <img class="thumbnails" alt="Imagen del producto" src="{{thumbnails}}">
            <h2>{{title}}</h2>
            <p class="description">{{description}}</p>
            <p>Precio: ${{price}}</p>
            <p>Stock:{{stock}}</p>
            <button class="add-to-cart">AÃ±adir al carrito</button>
        </div>
    {{/each}}
</div>


